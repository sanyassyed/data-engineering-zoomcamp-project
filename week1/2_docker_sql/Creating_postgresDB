# CREATING THE IMAGE WITH POSTGRES
docker run -it \
  -e POSTGRES_USER="root" \
  -e POSTGRES_PASSWORD="root" \
  -e POSTGRES_DB="ny_taxi" \
  -v C://Users//SANYA//Documents//PersonalProjects//Zoomcamp//data-engineering-zoomcamp-main//week_1_basics_n_setup//2_docker_sql//testdocker//ny_taxi_postgres_data:/var/lib/postgresql/data \
  -p 5432:5432 \
  postgres:13